//! # JavaScript Globals
//!
//! Global identifiers from different JavaScript environments
//!
//! Rust fork of <https://www.npmjs.com/package/globals>

use phf::{phf_map, Map};

pub static GLOBALS: Map<&'static str, Map<&'static str, bool>> = phf_map! {
{{#each envs }}
    "{{name}}" => phf_map! {
    {{#each vars}}
        "{{name}}" => {{writeable}},
    {{/each}}
    },
{{/each}}
};
